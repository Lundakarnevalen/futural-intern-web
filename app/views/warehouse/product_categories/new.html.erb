<h3>Ny varukategori</h3>
<%= form_for [@product_category] do |f| %>
  <div class="row">
    <div class="col-xs-12 col-sm-12 col-md-6">
      <div class="form-group">
        <%= f.label :name, "Namn" %>
        <%= f.text_field :name, class: "form-control" %>
      </div>
    </div>
  </div>  
  <%= f.submit "Spara", class: "btn btn-primary" %>
<% end %>
