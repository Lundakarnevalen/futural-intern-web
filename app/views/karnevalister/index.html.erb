<% content_for :head do %>
<%= stylesheet_link_tag    :karnevalister, :media => :all %>
<% end %>

<%= render partial: "shared/header", locals: {title: "Karnevalistan"} %>

<div class="container" id="content">
  <%= render 'search_form' %>

  <% if !@karnevalister.blank? %>
  <ul>
    <% @karnevalister.each do |k| %>
      <li>
        <a href=<%= url_for k %>>
          <%= "#{k.efternamn}, #{k.fornamn} (#{k.personnummer})" %>
        </a>
      </li>
    <% end %>
  </ul>
  <% end %>
</div>

