<div id="left" class="col-md-8">
<%= render partial: "shared/header", locals: {title: "Karnevalistan"} %>

  <%= render 'search_form' %>

  <% if !@karnevalister.blank? %>
  <ul>
    <% @karnevalister.each do |k| %>
      <li>
        <a href=<%= url_for k %>>
          <%= "#{k.efternamn}, #{k.fornamn} (#{k.personnummer})" %>
        </a>
      </li>
    <% end %>
  </ul>
  <% end %>
</div>

