  <div id="right" class="col-md-4">
    <!--Sektion-->
    <div id="sektion" class="toggled row">
      <div class="header col-xs-12">
        <div class="col-xs-10">MIN SEKTION</div>
        <div class="col-xs-2">
          <%=image_tag "hamburger-icon.svg", {width: 16} %>
        </div>
      </div>
      <ul class="nav">
        <li>
          <a href="#">KOMMER SNART</a>
        </li>
        <li>
          <a href="#">MASSOR MED SKOJS</a>
        </li>
      </ul>
    </div>
    <!--Admin-->
    <div id="sektion" class="toggled row">
      <div class="header col-xs-12">
        <div class="col-xs-10">ADMIN</div>
        <div class="col-xs-2">
          <%=image_tag "hamburger-icon.svg", {width: 16} %>
        </div>
      </div>
      <ul class="nav">
        <% if can? :checkout, Karnevalist %>
          <li><%= link_to 'Checka ut karnevalister', checkout_karnevalister_path %></li>
        <% end %>
        <%# Sektionsadmin stuff %>
        <% if current_user.is? :sektionsadmin and current_user.karnevalist? %>
          <% if can? :read, Karnevalist %>
            <li><%= link_to 'Karnelistan', Karnevalist %></li>
          <% end %>
          <% s = current_user.karnevalist.sektion %>
          <% if can? :read, s %>
            <li><%= link_to 'Administrera din sektion', s %></li>
          <% end %>
          <% if current_user.sektioner.count > 1 %>
            <li><%= link_to 'Sektionslistan', Sektion %></li>
          <% end %>
          <% #if can? :create, Post %>
          <% if false # Add posts later %>
            <li><%= link_to 'Nytt inlägg', new_post_path %></li>
          <% end %>
          <% if can? :create, Notification %>
            <li><%= link_to 'Skicka ut pushnotiser', new_notification_path %></li>
          <% end %>
        <% end %>
        <% # Exporter %>
        <% if current_user.can? :export_all, Karnevalist %>
          <li><%= link_to 'Exportera personuppgifter', export_all_karnevalister_path %></li>
        <% end %>
        <% # Checker %>
        <% if can? :check, Karnevalist %>
          <li><%= link_to 'Karnevalistkoll', check_karnevalister_path %></li>
        <% end %>
        <%# Admin stuff %>
        <% if current_user.is? :admin %>
          <% if can? :read, Karnevalist %>
            <li><%= link_to 'Karnelistan', Karnevalist %></li>
          <% end %>
          <% if can? :read, Sektion %>
            <li><%= link_to 'Sektionslistan', Sektion %></li>
          <% end %>
          <% #if can? :create, Post %>
          <% if false # Add posts later %>
            <li><%= link_to 'Nytt inlägg', new_post_path %></li>
          <% end %>
          <% if can? :create, Notification %>
            <li><%= link_to 'Skicka ut pushnotiser', new_notification_path %></li>
          <% end %>
        <% end %>
        <% if can? :create, Post %>
          <li><%= link_to 'Nytt inlägg', new_post_path %></li>
        <% end %>
      </ul>
    </div>
