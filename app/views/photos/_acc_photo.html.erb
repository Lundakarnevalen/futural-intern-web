<div class="row">
  <div class="col-sm-6 col-md-4 col-md-offset-4">
    <div class="thumbnail" data-no-turbolink>
    <a data-toggle="modal" data-target="#img-modal">
      <%= image_tag("#{photo.image.url}", class: "img-modal-toggle") %>
     </a>
      <div class="caption">
        <p>
        Av <%= "#{photo.karnevalist.fornamn}" %>
        <% if photo.official %>
          <span class="label label-success">Officiell fotograf</span>
        <% end %>
        </p>
        <p><%= "#{photo.caption}" %></p>
      </div>
    </div>
  </div>
</div>
