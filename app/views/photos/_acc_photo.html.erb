<div class="row">
  <div class="col-sm-6 col-md-4 col-md-offset-4">
    <div class="thumbnail">
     <%= image_tag(request.protocol + request.host_with_port + photo.image.thumb.url) %>
      <div class="caption">
        <h3>
        By <%= "#{photo.karnevalist.fornamn}" %>
        <% if photo.official %>
          <span class="label label-success">Officiell fotograf</span>
        <% end %>
        </h3>
      </div>
    </div>
  </div>
</div>
