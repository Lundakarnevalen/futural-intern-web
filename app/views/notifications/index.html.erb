<%= render partial: "shared/header", locals: {title: "Push-meddelanden"} %>

<% @notifications.each do |n| %>
    <div class="notification white-container col-xs-12">
	   <a href=<%= url_for n %>>
			<h1><%= n.title %></h1>
			<div class="notification_date"><%= n.created_at.strftime("%Y-%m-%d %H:%M") %></div>
		</a>
	</div>
<% end %>
